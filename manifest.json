{
  "manifest_version": 3,
  "name": "To-Do Make",
  "description": "Base Level Extension",
  "version": "0.2",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "hello.html",
    "default_icon": "images/logo128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "popup.js",
        "todo_make.js",
        "note.js"
      ]
    }
  ]
}